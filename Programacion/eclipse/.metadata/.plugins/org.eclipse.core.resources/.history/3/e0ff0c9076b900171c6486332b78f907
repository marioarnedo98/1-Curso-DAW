package bucles;

public class unit3_1 {
	public int a = 3;
	public int b = 12;
	public int c = 37;
	public static min(){
		int result = 0;
		if (a < b && a < c && b < c)
			result = a;
		else if (a < b && a < c && b > c)
			result = a;

		else if (a > b && a < c && b < c)
			result = b;
		else if (a < b && b > c && c < a)
			result = c;

		else if (a > b && b < c && a > c)
			result = b;
		else if (a > b && a > c && c < b)
			result = c;
		return=result
	}
}
