package lastexam;

public class Enemy extends Character {
private String default_scary_msg= "Do you want to know what the fear is?";
public Enemy(String name, String default_scary_msg, int secretWeapon){
	this.setName(name);
	this.default_scary_msg=default_scary_msg;
	this.setSecretWeapon(secretWeapon);
}
public void Scare() {
	System.out.println(default_scary_msg);
}
public void Scare (String name) {
	System.out.println(name+ " Scares you " + default_scary_msg);
}
}
